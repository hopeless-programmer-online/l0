{
  /* https://aka.ms/tsconfig.json */
  "include": [
    "./src/"
  ],
  "exclude": [
    "node_modules",
    "./src/**/**.test.ts"
  ],
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "lib": [
      "ESNext",
    ],
    "outDir": "dist",
    "allowJs": false,
    "declaration": true,
    "downlevelIteration": true,
    "isolatedModules": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
